<!DOCTYPE html>
<html lang="fr">
<%- include('partials/head.ejs'); %>
<body>
    <div class="fr-skiplinks">
        <nav class="fr-container" role="navigation" aria-label="AccÃ¨s rapide">
            <ul class="fr-skiplinks__list">
                <% if (locals.use_dashboard_layout) { %>
                    <li>
                        <a class="fr-link" href="#main">Contenu</a>
                    </li>
                <% } %>
                <li>
                    <a class="fr-link" href="#footer">Pied de page</a>
                </li>
            </ul>
        </nav>
    </div>
    <div id="loader-bar"></div>
    <%- include('partials/header.ejs'); %>
    <div class="ac-questionnaire">
        <div class="ac-notice fr-notice fr-notice--info ac-notice--info fr-m-2w">
          <div class="fr-container">
            <div class="fr-notice__body">
              <p>
                <span class="fr-notice__title ac-notice__title">
                  <a
                    target="_blank"
                    rel="noopener external"
                    title="Questionnaire de satisfaction ProConnect"
                    href="https://tally.so/r/mJkV0Y"
                    class="fr-notice__link"
                    >Quel est votre avis sur ProConnect ?</a
                  >
                </span>
              </p>
              <button
                title="Masquer le message"
                onclick="const notice = this.parentNode.parentNode.parentNode; notice.parentNode.removeChild(notice)"
                id="button-1299"
                class="fr-btn--close fr-btn"
              >
                Masquer le message
              </button>
            </div>
          </div>
        </div>
      </div>      
    <% if (locals.use_dashboard_layout) { %>
        <main role="main" id="main" tabindex="-1">
            <%- body; %>
        </main>
    <% } else { %>
        <main role="main" class="onboarding-container">
            <%- body; %>
            <div class="centered-container">
                <img src="/dist/<%= locals.illustration ? locals.illustration : 'illu-account.svg' ;%>" alt="" />
            </div>
        </main>
    <% } %>
    <%- include('partials/footer.ejs'); %>
    <div tabindex="-1" id="aria-live-message" aria-live="assertive" class="fr-sr-only"></div>
    <noscript><p><img src="https://stats.data.gouv.fr/matomo.php?idsite=85&amp;rec=1" style="border:0;" alt="" /></p></noscript>
  </body>
</html>
